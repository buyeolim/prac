{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Launch Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Node.js: Launch Current File",
            "program": "${file}",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node",
            "console": "integratedTerminal"
        },
        {
            "type": "java",
            "name": "Java: Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
    ]
}